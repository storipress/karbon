    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "@vueuse/math": "^10.1.2",
    "@vueuse/nuxt": "^10.1.2",
    "@zhead/schema": "^1.1.0",
    "chalk": "^5.3.0",
